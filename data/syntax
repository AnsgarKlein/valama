
method_call
method _ ( _ ?$parameters _ )

value
object|$method_call

if_statement
if _ ( _ $value _ ?$comparison _ )


# Parameters

parameters
$value _ ?, _ ?$parameters


parameters_decl
type _ * _ ?$parameters_decl_inner

parameters_decl_inner
, _ type _ ?$parameters_decl


# Comparisons (Either is or ==,<=,> etc)

comparison
$rel_comparison|$is_comparison

rel_comparison
$relational_expression _ $value

is_comparison
is _ type



# Keywords, operators and stuff

assign_operator
=|+=|-=||=|&=|^=|/=|*=|%=|<<=|>>=

relational_expression
==|>|<|>=|<=

access_keyword
public|private|internal
