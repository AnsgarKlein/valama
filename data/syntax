init_method
?_ $ReturnStatement|$AssignStatement|$CallStatement|$IfStatement|$DeclarationStatement|$SignalConnect

init_deep_space
?_ $UsingStatement



DeclarationStatement
$TypedDeclaration|$VarDeclaration

VarDeclaration
var _ *word $Assignment ?_ ;

TypedDeclaration
$Type _ *word ?$Assignment ?_ ;


UsingStatement
using _ %Namespace{child} ?$NamespaceChild{child} ?_ ;

NamespaceChild : parent
. {parent}>Namespace{child} ?$NamespaceChild{child}


IfStatement
if ?_ ( ?_ $Value ?_ ==|<|>|<=|>= ?_ $Value ?_ )

CallStatement
$MethodCall ?_ ;

AssignStatement
$Object $Assignment ?_ ;

Assignment
?_ = ?_ $Value|$NewExpression

ReturnStatement
return _ $Value|$NewExpression ?_ ;



NewExpression
new _ $Type ?_ ( ?$MethodParameters )



Type
$TypeClassStart|$TypeNamespaceStart

TypeClassStart
%Class{a} ?$TypeIterClass{a}

TypeNamespaceStart
%Namespace{a} $TypeIterNamespace{a}|$TypeIterClass{a}

TypeIterNamespace : parent
. {parent}>Namespace{child} $TypeIterNamespace{child}|$TypeIterClass{child}

TypeIterClass : parent
. {parent}>Class{child} ?$TypeIterClass{child}



SignalConnect
$SignalALL{@}

SignalIter : parent
. $SignalALL{parent}

SignalALL : parent
$SignalChildMethod{parent}|$SignalChildStuff{parent}|$SignalChildVariables{parent}|$SignalChildConnect{parent}

SignalChildStuff : parent
{parent}>Namespace{a}|{parent}>Class{a} $SignalIter{a}

SignalChildVariables : parent
{parent}>Variable{a}|{parent}>Property{a} $SignalIter{a}

SignalChildMethod : parent
{parent}>Method{a} ?_ $MethodCallBrackets{a} ?$SignalIter{a}

SignalChildConnect : parent
{parent}>Signal{a} .connect ?_ ( ?_ ) ?_ ;



MethodCall
$MethodALL{@}

MethodIter : parent
. $MethodALL{parent}

MethodALL : parent
$MethodChildMethod{parent}|$MethodChildStuff{parent}|$MethodChildVariables{parent}

MethodChildStuff : parent
{parent}>Namespace{a}|{parent}>Class{a} $MethodIter{a}

MethodChildVariables : parent
{parent}>Variable{a}|{parent}>Property{a} $MethodIter{a}

MethodChildMethod : parent
{parent}>Method{a} ?_ $MethodCallBrackets{a} ?$MethodIter{a}



Value
null|$ValueALL{@}

ValueIter : parent
. $ValueALL{parent}

ValueALL : parent
$ValueChildMethod{parent}|$ValueChildEnum{parent}|$ValueChildStuff{parent}|$ValueChildConstant{parent}|$ValueChildVariables{parent}

ValueChildStuff : parent
{parent}>Namespace{a}|{parent}>Class{a} $ValueIter{a}

ValueChildVariables : parent
{parent}>Variable{a}|{parent}>Property{a} ?$ValueIter{a}

ValueChildConstant : parent
{parent}>Constant

ValueChildEnum : parent
{parent}>Enum{child} . {child}>Constant

ValueChildMethod : parent
{parent}>Method{a} ?_ $MethodCallBrackets{a} ?$ValueIter{a}



MethodCallBrackets : method
?_ ( ?$MethodCallParam ?_ )

MethodCallParam
?_ $Value ?MethodCallParamIter

MethodCallParamIter
?_ , ?_ $Value ?MethodCallParamIter





Object
%Variable{a} ?$ChildObject{a}

ChildObject : parent
. $ChildVar{parent}|$ChildMeth{parent}|$ChildEnum{parent}

ChildVar : parent
{parent}>Variable{child} ?$ChildObject{child}

ChildMeth : parent
{parent}>Method{mth} ( ?$MethodParameters ) ?$ChildObject{mth}

ChildEnum : parent
{parent}>Enum{child} . {child}>Constant



MethodParameters
$Object ?$MethodParametersIter

MethodParametersIter
_ , _ $MethodParameters
